---
title: Фотоальбомы
layout: page
---

<div class="container">
	<div class="row">


	<% for document in @getCollection('gallery').toJSON(): %>
	<a href="<%= document.url %>" class="thumbnail">
		<div class="col-md-1">
	                <h2><%= document.title %></h2>
		                <img class="img-responsive" src="<%= @getThumbnail(document.img, "zoomcrop", { w: 100, h: 100 }) %>" alt="">

		</div>
</a>
	<% end %>
</div>

</div>
