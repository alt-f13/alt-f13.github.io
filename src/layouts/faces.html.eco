---
layout: default
---

<div class="container">
  <div class="row">
    <div class="col-md-4"><%= @document.title %></div>
    <div class="col-md-4"></div>
    <div class="col-md-4"></div>
  </div>
</div>
<%- @content %>

<% for file in @getDocument().getAssociatedFiles().toJSON(): %>
<p>
    <h3><%= file.title or file.name %></h3>
    <img src="<%= file.url %>" title="<%= file.title or file.name %>" />
</p>
<% end %>
